{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://**"
        }
      ]
    }
  ]
}
